{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": false,
    "assets": [
      {"include": "../certs/*", "outDir": "./dist/certs1"},
      {"include": "../certs", "outDir": "./dist/certs2"}
    ]
  }
}
